{
  "name": "graphin-site",
  "version": "1.1.0-beta.1",
  "description": "antv graphin site",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run site:develop",
    "site:develop": "gatsby develop --open",
    "site:build": "npm run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:gh-page": "gh-pages -d ./public -r https://github.com/antvis/graphin-1.x-site.git",
    "site:deploy": "npm run site:build && gh-pages -d ./public -r https://github.com/antvis/graphin-1.x-site.git"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/graphin"
  },
  "devDependencies": {
    "@antv/gatsby-theme-antv": "^0.11.7",
    "gatsby": "^2.17.7",
    "gh-pages": "^2.1.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@antv/graphin": "*",
    "@antv/graphin-components": "*",
    "antd": "^4.8.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-i18next": "^11.0.1"
  }
}
